<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<GridPane alignment="TOP_LEFT" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.AccountCreationController">
  <children>
    <TextField alignment="CENTER" disable="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-1.0" prefHeight="50.0" prefWidth="100.0" text="Konotart ?" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
    <Pane fx:id="helpPane" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="1">
      <children>
        <ImageView fitHeight="${helpPane.height}" fitWidth="${helpPane.width}" layoutX="-50.0" layoutY="0.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@images/cardIcon.jpg" />
          </image>
        </ImageView>
      </children>
    </Pane>
    <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#createGiroAccount" prefHeight="50.0" prefWidth="100.0" text="Girokonto" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
    <Button id="bankBookButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#createBankBookAccount" prefHeight="50.0" prefWidth="100.0" text="Sparbuch" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
    <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#createPremiumAccount" prefHeight="50.0" prefWidth="100.0" text="Premiumkonto" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
    <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#createMetalAccount" prefHeight="50.0" prefWidth="100.0" text="Metallkonto" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
    <Pane fx:id="helpPaneBottom" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="3">
      <children>
        <ImageView fitHeight="${helpPaneBottom.height}" fitWidth="${helpPaneBottom.width}" layoutX="0.0" layoutY="-25.0" onMouseClicked="#goBack" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@images/arrowLeftIcon.png" />
          </image>
        </ImageView>
      </children>
    </Pane>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>a
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
