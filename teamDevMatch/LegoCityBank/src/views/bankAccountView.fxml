    <Button mnemonicParsing="false" onAction="#deposit" prefHeight="61.0" prefWidth="172.0" text="Einzahlen" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
    <Button mnemonicParsing="false" onAction="#disburse" prefHeight="61.0" prefWidth="172.0" text="Auszahlen" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
    <Pane fx:id="helpPaneLeft" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="3">
      <children>
        <ImageView onMouseClicked="#goBack" fitHeight="${helpPaneLeft.height}" fitWidth="${helpPaneLeft.width}" layoutX="49.0" layoutY="19.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@images/arrowLeftIcon.png" />
